    <properties>
        <!-- 文件拷贝时的编码 -->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <!-- 编译时的编码 -->
        <maven.compiler.encoding>UTF-8</maven.compiler.encoding>
        <fileEncoding>UTF-8</fileEncoding>

        <maven.compiler.target>1.8</maven.compiler.target>
        <maven.compiler.source>1.8</maven.compiler.source>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <!--SpringBoot版本-->
        <SPRING_BOOT_STARTER_VERSION>2.3.7.RELEASE</SPRING_BOOT_STARTER_VERSION>
        <!--JSON解析-fastjson版本-->
        <FASTJSON_VERSION>1.2.70</FASTJSON_VERSION>

        <!--Bean映射-orika版本-->
        <ORIKA_VERSION>1.5.4</ORIKA_VERSION>
        <!--<JAVASSIST.VERSION>3.28.0-GA</JAVASSIST.VERSION>
        <PARANAMER.VERSION>2.8</PARANAMER.VERSION>-->

        <!--Bean校验框架-->
        <HIBERNATE_VALIDATOR_VERSION>6.1.0.Final</HIBERNATE_VALIDATOR_VERSION>

        <!--google-guava-->
        <GUAVA_VERSION>28.0-jre</GUAVA_VERSION>

        <!--jtlog-->
        <JTLOG_VERSION>1.8.4-RELEASE</JTLOG_VERSION>

        <!--jtcomponent-->
        <!-- <JTCOMPONENT_VERSION>1.5.3-RELEASE</JTCOMPONENT_VERSION>-->
        <JTCOMPONENT_VERSION>1.5.8-RELEASE</JTCOMPONENT_VERSION>

        <!--knife4j-spring-boot-starter-->
        <KNIE4J_BOOT_STARTER_VERSION>3.0.3</KNIE4J_BOOT_STARTER_VERSION>

        <!--commons-lang3-->
        <COMMONS_LANG3_VERSION>3.9</COMMONS_LANG3_VERSION>

        <!--JDBC-->
        <MYSQL_JDBC_VERSION>8.0.16</MYSQL_JDBC_VERSION>
        <!--druid版本-->
        <DRUID_BOOT_STARTER_VERSION>1.2.8</DRUID_BOOT_STARTER_VERSION>
        <!--数据库连接池hikaricp-->
        <HIKARICP_VERSION>5.0.0</HIKARICP_VERSION>

        <!--mybatis-plus-boot-starter-->
        <MYBATIS_PLUS_BOOT_STARTER_VERSION>3.4.2</MYBATIS_PLUS_BOOT_STARTER_VERSION>
        <MYBATIS_PLUS_GENERATOR_VERSION>3.4.1</MYBATIS_PLUS_GENERATOR_VERSION>
        <!--p6spy-数据库sql打印-->
        <P6SPY_VERSION>3.9.1</P6SPY_VERSION>

        <!--hutool-->
        <HUTOOL_VERSION>5.6.5</HUTOOL_VERSION>

        <!--debezium-->
        <DEBEZIUM_VERSION>1.6.1.Final</DEBEZIUM_VERSION>

        <!--event-bus-->
        <EVENT_BUS_VERSION>3.1.1</EVENT_BUS_VERSION>

        <!--图片编辑压缩thumbnailator-->
        <HUMBNAILATOR_VERSION>0.4.8</HUMBNAILATOR_VERSION>

    </properties>


    <dependencies>

        <!--TEST-->
        <!-- https://mvnrepository.com/artifact/junit/junit -->
